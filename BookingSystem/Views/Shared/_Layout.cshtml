<body id="page-top" class="index">
<div id="skipnav"><a href="#maincontent">Skip to main content</a></div>

<!-- Navigation -->
<nav id="mainNav" class="navbar navbar-default navbar-fixed-top navbar-custom">
    <div class="container">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header page-scroll">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span> Menu <i class="fa fa-bars"></i>
            </button>
            <!-- <a class="navbar-brand" href="#page-top">Start Bootstrap</a> -->
            @*<img src="img/logo.png" height="60">*@
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
                <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-area="" asp-controller="Bookings" asp-action="Create">Bookings</a></li>
                <li><a asp-area="" asp-controller="Seats" asp-action="Index">Seats</a></li>
                <li><a asp-area="" asp-controller="Shows" asp-action="Index">Shows</a></li>
                <li><a asp-area="" asp-controller="Performances" asp-action="Index">Performances</a></li>

                
            </ul>
        </div>
        <!-- /.navbar-collapse -->
    </div>
    <!-- /.container-fluid -->
</nav>

@*<div class="container body-content">*@

<!-- Header -->
    

<!-- Portfolio Grid Section -->
<section id="portfolio">
    <div class="container" id="maincontent=" tabindex="-1">
        @RenderBody()
		  
		 
		  
		  
    </div>
</section>


